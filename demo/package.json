{
  "name": "use-eye-dropper-demo",
  "version": "1.4.4",
  "homepage": "http://woofers.github.io/use-eye-dropper",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf .next out build use-eye-dropper",
    "dev": "NODE_ENV=development node server.js",
    "build": "yarn clean && next build && mv build use-eye-dropper && mkdir build && mv use-eye-dropper build",
    "start": "npx serve@latest build",
    "lint": "next lint",
    "test": "playwright test",
    "test:ci": "xvfb-run --auto-servernum -- yarn test:install",
    "test:install": "npx playwright install && yarn test:skip",
    "test:skip": "SKIP_BUILD=true yarn test",
    "format": "prettier --write \"{tests,src}/**/*.{js,mjs}\""
  },
  "dependencies": {
    "@stitches/react": "^1.2.8",
    "chroma-js": "^2.4.2",
    "esbuild": "^0.17.19",
    "framer-motion": "^10.12.16",
    "hast-util-to-html": "^8.0.4",
    "lowlight": "^2.9.0",
    "mdx-bundler": "^9.2.1",
    "next": "^13.4.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "use-eye-dropper": "../."
  },
  "devDependencies": {
    "@playwright/test": "1.21.1",
    "@svgr/webpack": "^8.0.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.4.4",
    "eslint-webpack-plugin": "^4.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^5.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "serve": "^14.2.0",
    "serve-handler": "^6.1.5"
  }
}
